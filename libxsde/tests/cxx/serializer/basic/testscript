:
{
  $* >>EOO
    <hello xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="hello.xsd">
      <greeting>Hello</greeting>
      <name>sun</name>
      <name>moon</name>
      <name>world</name>
    </hello>
    EOO
}
